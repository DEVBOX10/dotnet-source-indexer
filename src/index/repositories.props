<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <Repository Include="runtime">
      <Url>https://github.com/dotnet/runtime</Url>
      <Projects>
        src/coreclr/src/System.Private.CoreLib/System.Private.CoreLib.csproj;
        src/libraries/*/src/*.csproj;
        src/libraries/*/src/*.vbproj;
      </Projects>
      <SparseCheckout>
!src/installer/*
!*.ilproj
!*.il
!*.c
!*.h.in
!*.cpp
!*.hpp
!*.inl
!*.asm
!*.S
!*.inc
!*.rc
!docs/*
!CMakeLists.txt
!*.cmake
!*.lst
/*
      </SparseCheckout>
      <PrepareCommand>
        src\coreclr\build.cmd -skiptests -skipnative -linuxmscorlib &amp;&amp; libraries.cmd /p:BuildNative=false /p:BuildPackages=false -binarylog -os Linux
      </PrepareCommand>
    </Repository>
  </ItemGroup>
</Project>
